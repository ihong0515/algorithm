package algorithm2;

import java.util.*;

public class exam7 {
	
	
	public static void main(String[] args) {
		// 홍재영 알고리즘 1일차 공부입니다.
		exam7 exam7 = new exam7(); 
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		int [] arr = new int [n];
		for(int i =0; i < n; i++) {
			arr[i] = in.nextInt();
		}
		System.out.print(exam7.solution(n, arr));
	}
	public int  solution(int n,int [] arr) {
		int answer = 0;
		
		int score = 0;
		
		for(int i =0;  i < arr.length; i++) {
			
			if(arr[i] == 0) {
				score = 0;
			}
			
		}
		return answer;
	}	
}
