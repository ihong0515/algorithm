package algorithm2;

import java.util.*;

public class exam5 {
//	5. 소수(에라토스테네스 체)
//	설명
//
//	자연수 N이 입력되면 1부터 N까지의 소수의 개수를 출력하는 프로그램을 작성하세요.
//
//	만약 20이 입력되면 1부터 20까지의 소수는 2, 3, 5, 7, 11, 13, 17, 19로 총 8개입니다.
//
//
//	입력
//	첫 줄에 자연수의 개수 N(2<=N<=200,000)이 주어집니다.
//
//
//	출력
//	첫 줄에 소수의 개수를 출력합니다.
	public static void main(String[] args) {
		// 홍재영 알고리즘 1일차 공부입니다.
		exam5 exam5 = new exam5(); 
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		
		System.out.print(exam5.solution(n));
		
	}
	public int solution(int n) {
		int answer = 0;
		
		int num = 0;
		//배열의 인덱스반복 도릴떄다 0부터 시작해서 -1  까지만 돌아야 하는 거지 일반 차수면 상관 없잖아.
		for(int i = 1;  i <= n; i++) {
			if(i == 1) {
				
			}else if(i != 2 && i % 2 == 0) {
				
			}else if(i != 3 && i % 3 == 0) {
				
			}else if(i != 5 && i % 5 == 0) {
				
			}else if(i != 7 && i % 7 == 0) {
				
			}else {
				answer ++;
			}
				
		}
		return answer;
	}	
}
